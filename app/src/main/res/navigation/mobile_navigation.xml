<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.instanceservice.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.instanceservice.ui.profile.ProfileFragement"
        android:label="Profile"
        tools:layout="@layout/fragment_profile"/>
    <fragment
        android:id="@+id/nav_cars"
        android:name="com.example.instanceservice.ui.cars.MyCarsFragement"
        android:label="My Cars"
        tools:layout="@layout/fragment_my_cars"/>
    <fragment
        android:id="@+id/nav_service_history"
        android:name="com.example.instanceservice.ui.service_history.ServiceHistoryFragment"
        android:label="Service History"
        tools:layout="@layout/fragment_service_history"/>

    <fragment
        android:id="@+id/nav_sell_cars"
        android:name="com.example.instanceservice.ui.sell_cars.SellCarsFragment"
        android:label="Sell Cars"
        tools:layout="@layout/fragment_sell_cars"/>

    <fragment
        android:id="@+id/nav_about_us"
        android:name="com.example.instanceservice.ui.about_us.AboutUsFragment"
        android:label="About Us"
        tools:layout="@layout/fragment_about_us"/>

</navigation>